{
  "name": "Google Filter",
  "version": "0.1",
  "manifest_version": 2,
  "description": "get rid of redirect from google, open web page directly",
  "content_scripts": [{
    "matches": ["http://www.google.com/*", "https://www.google.com/*", "http://www.google.com.hk/*", "https://www.google.com.hk/*"],
    "js": ["strip.js"],
    "run_at": "document_end"
  }],
  "icons": {
    "48": "images/48.png",
    "128": "images/128.png"
  }
}
